// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Shop {
  id Int @id @default(autoincrement())
  name String @unique
  address String
  
}

model ShopProduct{
  id Int @id @default(autoincrement())
  shopId Int
  productId Int
}